DEBUG=false
MEMORY_LIMIT=1024M
CLIENT_RESPATH=resources_2/
CLIENT_DATAINI=DATA.INI
CLIENT_AUTOEXTRACT=false
CLIENT_ENABLESEARCH=false

# Compression settings
COMPRESSION_ENABLED=true
COMPRESSION_MIN_SIZE=1024
COMPRESSION_LEVEL=6
# LRU Cache Configuration
# Enable/disable file content caching for improved performance
CACHE_ENABLED=true
# Maximum number of files to keep in cache (recommended: 100-500)
CACHE_MAX_FILES=100
# Maximum memory usage for cache in MB (recommended: 128-512)
CACHE_MAX_MEMORY_MB=256
# Korean Path Mapping Configuration
# Enable path mapping for Korean filenames (CP949/EUC-KR)
PATH_MAPPING_ENABLED=true
# Path to the mapping file (JSON format)
PATH_MAPPING_FILE=path-mapping.json